{
  "author": "Dean Merchant",
  "name": "with-url-state",
  "version": "0.0.2",
  "dependencies": {
    "lodash": "~4.17.4",
    "query-string": "~5.0.0"
  },
  "peerDependencies": {
    "history": "4.x",
    "react": "15.x || 16.x",
    "react-dom": "15.x || 16.x"
  },
  "devDependencies": {
    "@types/enzyme": "3.1.0",
    "@types/history": "4.6.1",
    "@types/jest": "21.1.4",
    "@types/lodash": "4.14.77",
    "@types/query-string": "5.0.0",
    "@types/react": "16.0.14",
    "@types/react-dom": "16.0.1",
    "concurrently": "3.5.0",
    "enzyme": "3.1.0",
    "enzyme-adapter-react-16": "1.0.2",
    "history": "4.7.2",
    "jest": "21.2.1",
    "jsdom": "11.3.0",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-test-renderer": "16.0.0",
    "ts-jest": "21.1.3",
    "typescript": "2.5.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/index.ts"
    ],
    "coverageReporters": [
      "text"
    ],
    "mapCoverage": true,
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/setupTestFramework.js",
    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/src/**/?(*.)(spec|test).ts?(x)"
    ],
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    }
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "dist/withUrlState.js",
  "types": "dist/withUrlState.d.ts",
  "repository": {
    "type": "git",
    "url": "git@github.com:Dean177/with-url-state.git"
  },
  "scripts": {
    "build": "rm -rf dist && tsc",
    "build:dev": "tsc --noEmit --pretty --watch",
    "dev": "concurrently -n \"make,test\" \"npm run build:dev\" \"npm run test:dev\"",
    "test": "jest --coverage",
    "test:dev": "jest --watch",
    "prepublish": "npm run test && npm run build"
  }
}
